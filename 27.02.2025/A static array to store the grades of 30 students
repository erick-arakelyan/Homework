#include <iostream>

void inputGrades(int grades[], int size);

int main() {
    const int SIZE = 30; 
    int grades[SIZE];
    inputGrades(grades, SIZE);
    return 0;
}

void inputGrades(int grades[], int size) {
    std::cout << "Enter the grades of " << size << " students:\n";
    for (int i = 0; i < size; i++) {
        std::cout << "Grade " << i + 1 << ": ";
        std::cin >> grades[i];
    }
}
